{
    "version": 2,
    "public": true,
    "name": "ng-techverito",
    "alias": ["ngTechVerito"],
    "github": {
      "autoAlias": true
    },
    "builds": [
      {
        "src": "package.json", "use": "@now/static-build", 
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      { "src": "(.*\\..*)", "dest": "$1" },
      { "src": ".*", "dest": "/index.html"}
    ]
  }